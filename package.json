{
  "name": "pennyworth",
  "version": "0.0.4",
  "description": "a natural language templating engine for alfred",
  "main": "pennyworth-dist.js",
  "dependencies": {
    "babel": "^5.8.19",
    "natural": "^0.2.1",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "test": "gulp test && npm run coverage && npm run codeclimate && npm run rm",
    "coverage": "istanbul cover --lcov-only node_modules/.bin/_mocha -- test/test-debug.js",
    "debug": "gulp && mocha --debug-brk -R spec test/test-debug.js",
    "codeclimate": "cat coverage/lcov.info | codeclimate-test-reporter",
    "rm": "rimraf coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karimsa/pennyworth.git"
  },
  "keywords": [
    "natural",
    "language",
    "templating"
  ],
  "author": "Karim Alibhai",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/karimsa/pennyworth/issues"
  },
  "homepage": "https://github.com/karimsa/pennyworth#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.1.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-mocha": "^2.1.3",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "should": "^7.0.2"
  }
}
